#include<stdio.h>
int main(void)
{
	int i,n,c=0,j,flag=0;	int a[10];
	scanf("%d",&n);
    for(i=0;i<n;i++)
	  scanf("%d",&a[i]);
	  for(i=0;i<n;i++)
	  {for(j=0;j<n;j++)
	  c=0;
	    if(a[i]==a[j]);
	       c++;
	    if(c==a[j])
	      flag=1;
		  }
	  if(flag=1)
	  printf("%d ",c);
	  if(flag=0)  printf("-1");
 return 0;}
